public function destroysection($id)
    {
        $post = PostSection::find($id);
        $post->delete();
        $post = PostSection::where('parent', $id);
        $post->delete();
        // $message = array('message.level' => 'alert-success', 'message.content' => 'Post Sectin Deleted');
        return redirect()->back()->with('message', 'Sectin Deleted');
    }
